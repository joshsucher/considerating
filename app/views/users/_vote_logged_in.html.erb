<div id="new-vote-<%= @consideration.id %>">
	<%= form_for current_user.votes.build(:voted_id => @consideration.id), 
    	         :remote => true do |f| %>
		<%= f.hidden_field :voted_id %>
		<div class="rating_logged_in">
			<%= f.range_field :rating, :min => 1, :max => 10, :step => 0.1, :placeholder => 5 %>
			<div id="vote-container"><div class="vote-creepy">creepy</div> 
			<div class="vote-considerate">considerate</div></div>
		</div>
		<div class="actions vote-button"><%= f.submit "vote", :class => "btn2 span2", :id => "vote-button" %></div>
	<% end %>
</div>